<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hitrust.acl.model.DayCustCont" table="DAY_CUST_CONT">
          <composite-id name="id" class="com.hitrust.acl.model.base.AbstractDayCustCont$Id">
            <key-property name="setlDate" type="java.lang.String">
                <column name="SETL_DATE" />
            </key-property>
            <key-property name="stts" type="java.lang.String">
                <column name="STTS" />
            </key-property>
        </composite-id>
        <property name="totlCont" type="java.lang.Integer">
            <column name="TOTL_CONT" />
        </property>
        <property name="mdfyDttm" type="java.util.Date">
            <column name="MDFY_DTTM" />
        </property>
    </class>
</hibernate-mapping>
