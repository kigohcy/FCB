                <column name="ACNT_INDT" />
            </key-property>
            <key-property name="trnsMnth" type="java.lang.String">
                <column name="TRNS_MNTH" />
            </key-property>
        </composite-id>

        <property name="custId" type="java.lang.String">
            <column name="CUST_ID" />
        </property>
        <property name="ecId" type="java.lang.String">
            <column name="EC_ID" />
        </property>
        <property name="ecUser" type="java.lang.String">
            <column name="EC_USER" />
        </property>
        <property name="realAcnt" type="java.lang.String">
            <column name="REAL_ACNT" />
        </property>
        <property name="mnthCont" type="java.lang.Long">
            <column name="MNTH_CONT" precision="15" scale="0" />
        </property>
        <property name="custSerl" type="java.lang.String">
            <column name="CUST_SERL" />
        </property>
    </class>
</hibernate-mapping>
